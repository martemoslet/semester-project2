{
  "name": "semester-project2",
  "version": "0.0.5",
  "description": "Auction website",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.js",
    "prepare": "husky install",
    "build": "sass src/scss:dist/css",
